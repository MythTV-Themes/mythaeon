<mythuitheme>
    <window name="osd_debug">
        <area>0,0,1920,188></area>

        <imagetype name="osd_bgd">
           <filename>images/osd/codec_top.png</filename>
           <area>0,0,1920,188></area>
           <preserveaspect>no</preserveaspect>
        </imagetype>
        <group name="osd_debug_group">
           <area>100,10,1920,188></area>
	        <textarea name="filename">
	            <font>Font_Clock_Bold</font>
	            <area>20,10,1880,40</area>
	            <align>left,vcenter</align>
	            <template>Filename: %filename%</template>
	        </textarea>
	
	        <textarea name="storagerate">
	            <font>Font_Clock</font>
	            <area>20,50,450,40</area>
	            <align>left,vcenter</align>
	            <template>Storage to Buffer: %storagerate%</template>
	        </textarea>
	        <textarea name="decoderrate">
	            <font>Font_Clock</font>
	            <area>20,90,450,40</area>
	            <align>left,vcenter</align>
	            <template>Buffer to Decoder: %decoderrate%</template>
	        </textarea>
	        <textarea name="bufferavail">
	            <font>Font_Clock</font>
	            <area>20,130,450,40</area>
	            <align>left,vcenter</align>
	            <template>Available Buffer: %BUFFERAVAIL% of %BUFFERSIZE%Mb</template>
	        </textarea>
	
	        <textarea name="videowidth">
	            <font>Font_Clock</font>
	            <area>490,50,450,40</area>
	            <align>left,vcenter</align>
	            <template>Video: %VIDEOWIDTH%x%VIDEOHEIGHT%@%VIDEOFRAMERATE%fps</template>
	        </textarea>
	        <textarea name="videocodec">
	            <font>Font_Clock</font>
	            <area>490,90,450,40</area>
	            <align>left,vcenter</align>
	            <template>Codec/Dec: %VIDEOCODEC% %VIDEODECODER%</template>
	        </textarea>
	        <textarea name="load">
	            <font>Font_Clock</font>
	            <area>490,130,450,40</area>
	            <align>left,vcenter</align>
